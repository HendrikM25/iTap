        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/rank9"
        app:layout_constraintTop_toBottomOf="@+id/rank9" />

    <TextView
        android:id="@+id/rank1Score"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/emptyHighscore"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/rank1Name"
        app:layout_constraintEnd_toEndOf="@+id/scoreImage"

<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="center"
    tools:context=".HighscoresActivity">

    <ImageView
        android:id="@+id/iTapHighscoresLogo"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/highscoresLogoDescription"
        app:layout_constraintBottom_toTopOf="@+id/rankImage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/itap_highscores_logo" />

    <ImageView
        android:id="@+id/rankImage"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="@string/rankImageDescription"
        app:layout_constraintEnd_toStartOf="@+id/scoreImage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iTapHighscoresLogo"
        app:srcCompat="@drawable/highscores_rank" />

    <ImageView
        android:id="@+id/scoreImage"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="65dp"
        android:contentDescription="@string/scoreImageDescription"
        app:layout_constraintBottom_toBottomOf="@+id/rankImage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rankImage"
        app:layout_constraintTop_toTopOf="@+id/rankImage"
        app:srcCompat="@drawable/highscores_score" />

    <TextView
        android:id="@+id/rank1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:text="@string/rank1"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/rank2"
        app:layout_constraintStart_toStartOf="@+id/rankImage"
        app:layout_constraintTop_toBottomOf="@+id/rankImage" />

    <TextView
        android:id="@+id/rank2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"

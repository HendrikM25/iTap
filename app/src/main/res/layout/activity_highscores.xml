        app:layout_constraintBottom_toTopOf="@+id/rank3"
        app:layout_constraintStart_toStartOf="@+id/rank1"
        app:layout_constraintTop_toBottomOf="@+id/rank1" />

    <TextView
        android:id="@+id/rank3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/rank3"
        app:layout_constraintBottom_toTopOf="@+id/rank4"
        app:layout_constraintStart_toStartOf="@+id/rank2"
        app:layout_constraintTop_toBottomOf="@+id/rank2" />

    <TextView
        android:id="@+id/rank4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
